    1  wget -q -O - https://apt.grafana.com/gpg.key | gpg --dearmor | sudo tee /etc/apt/keyrings/grafana.gpg > /dev/null
    2  sudo apt update 
    3  ls
    4  sudo apt install -y apt-transport-https software-properties-common wget
    5  sudo mkdir -p /etc/apt/keyrings/
    6  echo "deb [signed-by=/etc/apt/keyrings/grafana.gpg] https://apt.grafana.com stable main" | sudo tee -a /etc/apt/sources.list.d/grafana.list
    7  cat /etc/apt/keyrings/grafana.gpg 
    8  sudo apt install grafana
    9  sudo snap install grafana
   10  sudo grafana-server -v
   11  sudo apt update 
   12  sudo apt purge grafana
   13  sudo apt install grafana
   14  sudo grafana-server -v
   15  sudo systemctl enable grafana-server.service 
   16  sudo systemctl start grafana-server.service 
   17  sudo systemctl status grafana-server.service 
   18  ps aux | grep grafana 
   19  history > grafana 
